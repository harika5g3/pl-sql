
create view cal as select deptno, min(esal) as "MIN SAL", max(esal) as "MAX SAL", avg(esal) as "AVG SAL" from emp group by deptno;

SELECT rollno,sname, CONCAT('19K61A0',rollno) reg_id FROM student order by rollno;


select eno, ename, esal, edept from empno order by esal desc fetch first row only;
